		<span class="kd">var</span> <span class="nx">square</span> <span class="o">=</span> <span class="nx">Event</span><span class="p">.</span><span class="nx">getTarget</span><span class="p">(</span><span class="nx">e</span><span class="p">);</span>
		
		<span class="c">// Stop the event to prevent a selection</span>
