	   <span class="c">// Overlay closure</span>
      <span class="k">this</span><span class="p">.</span><span class="nx">oOverlay</span><span class="p">.</span><span class="nx">hide</span><span class="p">();</span>
       
	   <span class="c">// SetValue</span>
		<span class="kd">var</span> <span class="nx">color</span> <span class="o">=</span> <span class="nx">Dom</span><span class="p">.</span><span class="nx">getStyle</span><span class="p">(</span><span class="nx">square</span><span class="o">,</span><span class="s1">&#39;background-color&#39;</span><span class="p">);</span>
